<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Luma Test Regression PW Suite" thread-count="5"
	parallel="tests" verbose="4">

	<listeners>
		<listener
			class-name="com.qa.luma.listeners.ExtentReportListener" />

	</listeners>

	<test name="Luma Home Page Test_chrome">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.qa.luma.tests.HomePageTest"></class></classes>
	</test>
	<test name="Luma Login Page Test_chrome">
		<parameter name="browser" value="firefox"></parameter>
		<classes>
			<class name="com.qa.luma.tests.LoginPageTest"></class></classes></test>




</suite>